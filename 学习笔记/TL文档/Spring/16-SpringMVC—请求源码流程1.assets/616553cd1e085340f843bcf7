<script type="text/javascript">
console.log("流程图预览88888")
Designer.events.addEventListener("resetBrokenLinker", function(old){
var lineJumps = Model.define.page.lineJumps;
if(lineJumps != null && lineJumps == false && old == true){
var linkers = Model.getLinkers("broken");
for(var i = 0, len = linkers.length; i < len; i++){
var linker = linkers[i];
Designer.painter.renderLinker(linker);
Model.intersection = {};
}
return;
}else if(old == lineJumps || lineJumps == null){
return;
}else if(old == null && lineJumps == false){
return;
}
var shapeLength = Model.orderList.length;
if(shapeLength > 400){
Model.intersection = {};
if(shapeLength <= 1000){
UI.showTip("<@i18n resource='diagraming.linejumps.tip'/>");
}
return;
}
var linkers = Model.getLinkers("broken");
var maps = {},maps1 = {},maps2 = {};
for(var i = 0, len = linkers.length; i < len; i++){
var linker = linkers[i];
maps[linker.id] = Utils.getLinesFromLinker(linker);
}
var crossObj = [];
for(var key in maps){
var list = maps[key];
for(var key_ in maps){
var list_ = maps[key_];
if(key_ == key){
continue;
}
for(var i = 0; i < list.length; i++){
var p = list[i];
var p1 = {
x:p.x1,
y:p.y1
}
var p2 = {
x:p.x2,
y:p.y2
}
for(var j = 0; j < list_.length; j++){
var crossLine = {};
var p_ = list_[j];
var p3 = {
x:p_.x1,
y:p_.y1
}
var p4 = {
x:p_.x2,
y:p_.y2
}
if(Utils.checkCross(p1, p2, p3, p4)){
var point = Utils.intersection(p1, p2, p3, p4);
if(Math.abs(point.x - p1.x) < 12 && Math.abs(point.y - p1.y) < 8){
continue;
}
if(Math.abs(point.x - p2.x) < 12 && Math.abs(point.y - p2.y) < 8){
continue;
}
if(Math.abs(point.x - p3.x) < 20 && Math.abs(point.y - p3.y) < 8){
continue;
}
if(Math.abs(point.x - p4.x) < 20 && Math.abs(point.y - p4.y) < 8){
continue;
}
point.y = Math.round(point.y);
var ckey = key, idx = i;
if(p1.y != p2.y && p3.y == p4.y){
ckey = key_;
idx = j;
}
var exists = false;
if(crossObj.length > 0){
for(var x = 0; x < crossObj.length; x++){
var cObj = crossObj[x];
if(cObj.curveId && cObj.curveId == ckey){
crossLine = cObj;
exists = true;
break;
}
}
}
if(crossLine["points"] == null){
crossLine["points"] = [];
}
crossLine["points"].push(point);
if(!exists){
crossLine["curveId"] = ckey;
crossObj.push(crossLine);
}
}
}
}
}
}
var temp = Utils.copy(Model.intersection);
Model.intersection = {};
if(crossObj.length > 0){
var compare = function () {
return function (obj1, obj2) {
if (obj1.x < obj2.x) {
return -1;
} else if (obj1.x > obj2.x) {
return 1;
}
}
}
for(var i = 0; i < crossObj.length; i++){
var line = crossObj[i];
var linker = Model.getShapeById(line.curveId);
line.points = line.points.sort(compare());
for (var j = 0; j < line.points.length - 1; j++) {
if (line.points[j].x == line.points[j+1].x && line.points[j].y == line.points[j+1].y) {
line.points.splice(j, 1)
j--
}
}
Designer.painter.renderLinker(linker, null, null, line);
Model.intersection[line.curveId] = line;
}
}
if(!$.isEmptyObject(temp)){
for(var key in temp){
if(Model.intersection[key] != null){
continue;
}
var linker = Model.getShapeById(key);
if(linker == null){
continue;
}
Designer.painter.renderLinker(linker);
}
}
crossObj = {};
maps = null;
});
