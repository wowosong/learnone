var aClick = function(e){
if(e.path) {
for(var i=0; i < e.path.length; i++) {
if(e.path[i].localName == "a" && e.path[i].href && e.path[i].href != ""){
e.stopPropagation();
e.preventDefault();
window.location.href = e.path[i].href;
}
}
}
